jQuery(document).ready(function ($) {
    $('.star_rating_display').raty({
        readOnly: true,
        score: function () {
            return $(this).attr('data-rating');
        },
        half: true,
        precision: true,
        starOff: About_object.current_url + '/images/star-off.png',
        starOn: About_object.current_url + '/images/star-on.png',
        starHalf: About_object.current_url + '/images/star-half.png',
    });

    $("#star_rate").raty({
        half: true,
        precision: true,
        starOff: About_object.current_url + '/images/star-off.png',
        starOn: About_object.current_url + '/images/star-on.png',
        starHalf: About_object.current_url + '/images/star-half.png',
        score: function () {
            return $(this).attr('data-rating');
        },
        click: function (score, evt) {
            $("#star_val").val(score);
        }
    });
});